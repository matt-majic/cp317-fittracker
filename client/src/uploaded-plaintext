# FitTrackerLogo.tsx

"use client";

import React from "react";

interface FitTrackerLogoProps {
  className?: string;
}

const FitTrackerLogo: React.FC<FitTrackerLogoProps> = ({ className = "" }) => {
  return (
    <img
      src="https://cdn.builder.io/api/v1/image/assets/TEMP/8e8d20912f4e1f3b5c8855fb0b71c77626ae65c9"
      alt="FITTracker"
      className={`text-4xl italic text-center text-lime-300 capitalize h-[60px] w-[277px] max-md:w-60 max-md:text-4xl max-md:h-[52px] max-sm:text-3xl max-sm:h-[43px] max-sm:w-[200px] ${className}`}
    />
  );
};

export default FitTrackerLogo;

# LogoScreen.tsx 

