const LogoScreen: React.FC = () => {
  return (
    <section className="flex justify-center items-center w-full min-h-screen rounded-3xl border border-white border-solid bg-neutral-800">
      <FitTrackerLogo />
    </section>
  );
};

export default LogoScreen;

